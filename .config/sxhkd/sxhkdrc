# open terminal
super + Return
	termite

# open program launcher
super + shift + Escape
	rofi -show drun

# kill process
super + w
	bspc node --kill

# focus on the next node in the current desktop
alt + Tab
	bspc node --focus next.local

# switch desktops
super + {1-9}
  bspc desktop --focus \^{1-9}
